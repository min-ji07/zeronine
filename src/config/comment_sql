#댓글용 DB 생성
CREATE TABLE DQA(
	DNO 		NUMBER 			PRIMARY KEY,
	BNO 		NUMBER			NOT NULL,
	DCONTENT 	VARCHAR2(2000)	NOT NULL,
	USERID		VARCHAR2(15)	NOT NULL,
	DDATE		DATE,
	REF_DNO		NUMBER,
	CLEVEL		NUMBER,
	CWRITER		VARCHAR2(20),
	LOC			NUMBER
);

#댓글용 시퀀스 생성
CREATE SEQUENCE LIKEY_SEQ 
START WITH 1 
INCREMENT BY 1;
COMMIT; 